<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ershixiong.ai.infrastructure.repository.mybatis.mapper.CityMapper">
    <select id="findByNameLike" resultType="com.ershixiong.ai.infrastructure.repository.mybatis.dataobject.CityDO">
        SELECT id, name, countrycode, district, population
        FROM city
        WHERE name LIKE #{name}
    </select>
</mapper>
